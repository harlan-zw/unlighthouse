{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    }
  },
  "include": [
    "*.ts",
    "*.vue",
    "**/*.ts",
    "**/*.vue",
    "**/*.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".nuxt"
  ]
}

{
  "name": "@unlighthouse/core",
  "version": "0.4.4",
  "description": "Scan your entire website with Google Lighthouse.",
  "license": "MIT",
  "homepage": "https://github.com/harlan-zw/unlighthouse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/unlighthouse.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/unlighthouse/issues"
  },
  "keywords": [
    "lighthouse",
    "audit",
    "seo",
    "performance",
    "speed"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependenciesMeta": {
    "puppeteer": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/wrap-ansi": "^8.0.1",
    "@unlighthouse/client": "workspace:../client",
    "@unrouted/core": "0.5.0",
    "@unrouted/fetch": "0.5.0",
    "@unrouted/plugins": "0.5.0",
    "@unrouted/preset-api": "0.5.0",
    "@unrouted/preset-node": "0.5.0",
    "axios": "1.1.3",
    "boxen": "^7.0.0",
    "chalk": "^4.1.2",
    "cheerio": "1.0.0-rc.12",
    "chrome-launcher": "^0.15.1",
    "consola": "^2.15.3",
    "defu": "^6.1.1",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.3",
    "h3": "^1.0.2",
    "hookable": "^5.4.2",
    "launch-editor": "^2.6.0",
    "lighthouse": "^9.6.8",
    "minimist": "^1.2.7",
    "mlly": "^1.0.0",
    "object-hash": "^3.0.0",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-core": "^19.4.1",
    "regexparam": "^2.0.1",
    "sanitize-filename": "^1.6.3",
    "sitemapper": "^3.2.6",
    "slugify": "^1.6.5",
    "ufo": "^1.0.1",
    "unconfig": "^0.3.7",
    "unctx": "^2.1.1",
    "wrap-ansi": "^8.0.1",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/object-hash": "^2.2.1",
    "@types/ws": "^8.5.3",
    "lodash-es": "^4.17.15"
  },
  "pnpm": {
    "packageExtensions": {
      "puppeteer-cluster": {
        "peerDependencies": {
          "puppeteer-core": "*"
        },
        "peerDependenciesMeta": {
          "puppeteer": {
            "optional": true
          }
        }
      }
    }
  }
}

{
  "name": "@unlighthouse/cli",
  "version": "0.5.0",
  "description": "CLI for Unlighthouse",
  "license": "MIT",
  "homepage": "https://github.com/harlan-zw/unlighthouse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/unlighthouse.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/unlighthouse/issues"
  },
  "keywords": [
    "unlighthouse",
    "cli"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/cli.mjs"
    },
    "./ci": {
      "import": "./dist/ci.mjs"
    }
  },
  "main": "dist/cli.mjs",
  "module": "dist/cli.mjs",
  "bin": {
    "unlighthouse-ci": "bin/unlighthouse-ci.cjs"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@unlighthouse/core": "workspace:../core",
    "@unlighthouse/server": "workspace:../server",
    "better-opn": "^3.0.2",
    "cac": "^6.7.14",
    "consola": "^2.15.3",
    "defu": "^6.1.2",
    "fs-extra": "^11.1.0",
    "globby": "^13.1.3",
    "lodash-es": "^4.17.21"
  }
}

{
  "name": "@unlighthouse/cli",
  "type": "module",
  "version": "0.17.4",
  "description": "CLI for Unlighthouse",
  "license": "MIT",
  "homepage": "https://github.com/harlan-zw/unlighthouse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/unlighthouse.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/unlighthouse/issues"
  },
  "keywords": [
    "lighthouse",
    "audit",
    "seo",
    "performance",
    "cli",
    "unlighthouse"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/cli.mjs"
    },
    "./ci": {
      "import": "./dist/ci.mjs"
    }
  },
  "main": "dist/cli.mjs",
  "module": "dist/cli.mjs",
  "bin": {
    "unlighthouse-ci": "bin/unlighthouse-ci.mjs"
  },
  "files": [
    "*.d.ts",
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "obuild",
    "stub": "obuild --stub"
  },
  "dependencies": {
    "@lhci/utils": "catalog:dependencies",
    "@unlighthouse/client": "workspace:*",
    "@unlighthouse/server": "workspace:*",
    "better-opn": "catalog:dependencies",
    "unlighthouse": "workspace:*",
    "cac": "catalog:dependencies",
    "consola": "catalog:dependencies",
    "defu": "catalog:dependencies",
    "fs-extra": "catalog:dependencies",
    "globby": "catalog:dependencies",
    "lodash-es": "catalog:dependencies",
    "pathe": "catalog:dependencies",
    "std-env": "catalog:dependencies"
  }

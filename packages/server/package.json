{
  "name": "@unlighthouse/server",
  "type": "module",
  "version": "0.16.3",
  "description": "Server for Unlighthouse",
  "license": "MIT",
  "homepage": "https://github.com/harlan-zw/unlighthouse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/unlighthouse.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/unlighthouse/issues"
  },
  "keywords": [
    "unlighthouse",
    "server"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@unlighthouse/core": "workspace:*",
    "h3": "^1.15.1",
    "listhen": "^1.9.0"
  }
}
